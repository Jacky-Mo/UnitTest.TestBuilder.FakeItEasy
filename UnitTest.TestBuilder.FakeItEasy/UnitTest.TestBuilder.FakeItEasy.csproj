<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;netstandard2.0</TargetFrameworks>
    <Authors>Jacky Mo</Authors>
    <Description>A test library that helps to write unit tests faster by abstractin out the boilerplate codes and let the developers focus on the actual logics of the unit tests instead.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/Jacky-Mo/UnitTest.TestBuilder.FakeItEasy</RepositoryUrl>
    <PackageTags>unittest;testbuilder;fakeiteasy,builder;builder-pattern</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FakeItEasy" Version="5.5.0" />
    <PackageReference Include="UnitTest.TestBuilder.Core" Version="1.0.1" />
  </ItemGroup>

</Project>
